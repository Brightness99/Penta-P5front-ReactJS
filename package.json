    "classnames": "^2.2.5",
    "core-decorators": "^0.17.0",
    "core-js": "^2.4.1",
    "ease-component": "^1.0.0",
    "fbjs": "^0.8.12",
    "history": "^4.6.3",
    "inputmask": "^3.3.7",
    "inscricaoestadual": "0.0.5",
    "intl": "^1.2.5",
    "mobile-detect": "^1.3.6",
    "modernizr": "^3.5.0",
    "moment": "^2.18.1",
    "rc-collapse": "^1.7.6",
    "react": "^15.6.1",
    "react-addons-shallow-compare": "^15.6.0",
    "react-credit-cards": "^0.4.1",
    "react-dates": "^12.5.0",
    "react-dom": "^15.6.1",
    "react-facebook": "^4.0.16",
    "react-facebook-login": "3.6.2",
    "react-google-login": "2.9.3",
    "react-helmet": "^5.2.0",
    "react-history": "^0.18.2",
    "react-inlinesvg": "^0.6.0",
    "react-load-script": "0.0.6",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-rte": "^0.15.0",
    "react-slick": "^0.14.11",
    "react-transition-group": "^2.2.0",
    "redux": "^3.7.1",
    "redux-observable": "^0.14.1",
    "redux-persist": "^4.8.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "rxjs": "^5.4.1",
    "sass-mediaqueries": "^1.6.1",
    "scroll": "^2.0.0",
