{
  "name": "alpha-frontend",
  "version": "0.1.0",
  "description": "Printi Alpha Front-End Application",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "start": "node config/webpack.server.js",
    "lint": "eslint  --ext .js --ext .jsx app/scripts config test",
    "lint:styles": "stylelint app/styles/**/*.scss",
    "flow": "flow check",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ui": "nightwatch -c test/lib/nightwatch.conf.js",
    "test:ui:start": "npm start -- test:ui",
    "precommit": "node config/tools commits && npm run lint",
    "postmerge": "node config/tools update && npm update",
    "fulltest": "npm run precommit && npm run flow && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/printi/alpha-frontend.git"
  },
  "keywords": [
    "printi",
    "react",
    "redux",
    "rxjs"
  ],
  "author": "Printi",
  "license": "MIT",
  "homepage": "https://www.printi.com.br",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "chalk": "^1.1.3",
    "chromedriver": "^2.30.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "del": "^2.2.2",
    "enzyme": "^2.9.1",
    "esdoc": "^0.5.2",
    "esdoc-es7-plugin": "0.0.3",
