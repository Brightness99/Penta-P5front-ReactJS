    "extract-text-webpack-plugin": "^2.1.2",
    "fetch-mock": "^5.12.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.45.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.13.4",
    "image-webpack-loader": "^3.3.0",
    "imports-loader": "^0.7.1",
    "jest-cli": "^19.0.2",
    "jsdom": "^10.1.0",
    "json-loader": "^0.5.4",
    "markdown-loader": "^2.0.0",
    "modernizr-loader": "^1.0.1",
    "nightwatch": "^0.9.16",
    "node-sass": "^4.5.2",
    "null-loader": "^0.1.1",
    "offline-plugin": "^4.7.0",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^3.0.0-beta",
    "reactotron-react-js": "^1.11.1",
    "reactotron-redux": "^1.11.2",
    "redux-diff-logger": "0.0.9",
    "redux-logger": "^3.0.1",
    "redux-mock-store": "^1.2.3",
    "rsync": "^0.6.1",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.0",
    "selenium-server-standalone-jar": "^3.4.0",
    "source-map-support": "^0.4.15",
    "style-loader": "^0.17.0",
    "stylelint": "^7.11.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-order": "^0.4.4",
    "stylelint-scss": "^1.4.4",
    "url-loader": "^0.5.9",
